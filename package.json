{
  "name": "react-seedling",
  "version": "1.0.0",
  "author": "@swaggerloaf",
  "description": "Starter react project using babel, webpack, bootstrap and VS Code",
  "scripts": {
    "start": "npm-run-all --parallel start2  start-mockapi",
    "start2": "node_modules/.bin/webpack-dev-server",
    "build": "node_modules/.bin/webpack --progress",
    "test": "jasmine",
    "generate-mock-data": "node buildScripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch db.json --port 3001",
    "mock": "json-server --watch db.json --port 3001"
  },
  "license": "MIT",
  "repository": "https://github.com/swaggerloaf/react-seedling-basic",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "bootstrap": "^4.3.1",
    "immer": "^2.1.1",
    "prop-types": "^15.7.2",
    "react": "next",
    "react-dom": "next",
    "react-redux": "6.0.1",
    "react-router-dom": "next",
    "react-toastify": "4.5.2",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "4.0.0",
    "font-awesome": "^4.7.0"
  },
  "devDependencies": {
    "@babel/core": "~7.3.4",
    "@babel/plugin-proposal-class-properties": "~7.3.4",
    "@babel/plugin-syntax-dynamic-import": "~7.2.0",
    "@babel/plugin-transform-runtime": "~7.3.4",
    "@babel/preset-react": "~7.0.0",
    "@babel/register": "~7.0.0",
    "@babel/preset-env": "~7.3.4",
    "@types/react": "~16.8.7",
    "@types/react-dom": "~16.8.2",
    "babel-eslint": "~10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "~2.1.1",
    "node-sass": "~4.11.0",
    "sass-loader": "7.1.0",
    "eslint": "~5.15.1",
    "eslint-plugin-react-hooks": "~1.5.0",
    "file-loader": "~3.0.1",
    "prettier": "^1.16.4",
    "style-loader": "~0.23.1",
    "webpack": "~4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-bundle-analyzer": "~3.1.0",
    "json-server": "~0.14.2",
    "json-schema-faker": "~0.5.0-rc16",
    "npm-run-all": "^4.1.5",
    "enzyme": "~3.9.0",
    "jest": "~24.3.1",
    "jest-cli": "~24.3.1",
    "jest-haste-map": "^24.3.1",
    "jest-resolve": "^24.3.1",
    "babel-jest": "^24.3.1"
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "react",
    "redux",
    "babel",
    "webpack",
    "jest"
  ],
  "eslintConfig": {
    "plugins": [
      "react-hooks"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "warn",
      "react/prop-types": "warn",
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-runtime"
    ]
  },
  "jest": {
    "setupFiles": [
      "./tools/testSetup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/fileMock.js",
      "\\.(css|less)$": "<rootDir>/tools/styleMock.js"
    }
  }
}
